{
  "name": "zoonotify-server",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "start": "node ./lib/main.js",
    "serve": "npx nodemon -e ts --exec \"npm run serve:pipe\"",
    "debug": "npx nodemon -e ts --exec \"npm run debug:pipe\"",
    "serve:pipe": "npm run build && node ./lib/main.js",
    "debug:pipe": "npm run build && node --inspect-brk ./lib/main.js",
    "cp:json": "npx copyfiles -u 1 ./src/**/*.json ./lib",
    "cp:all": "npm run cp:json",
    "tsc": "tsc",
    "build": "npm run tsc && npm run cp:all",
    "prebuild": "npm run clean",
    "clean": "npx rimraf lib/*",
    "lint": "tslint --project tsconfig.json --fix -c tslint.json 'src/**/*.ts'",
    "pretty": "prettier --write \"src/**/*.ts\""
  },
  "resolutions": {
    "tar": "4.4.19"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "./hooks/commit-msg ./.git/COMMIT_EDITMSG"
    }
  },
  "lint-staged": {
    "linters": {
      "src/**/*.ts": [
        "npm run pretty",
        "npm run lint",
        "git add"
      ]
    },
    "ignore": [
      "src/**/__mocks__/*.ts",
      "src/**/__tests__/*.ts"
    ]
  },
  "private": true,
  "dependencies": {
    "app-root-dir": "^1.0.2",
    "compression": "^1.7.3",
    "config": "^1.29.4",
    "core-js": "^2.6.12",
    "cors": "^2.8.4",
    "express": "^4.15.4",
    "helmet": "^3.23.3",
    "inversify": "^5.1.1",
    "inversify-express-utils": "^6.3.2",
    "jsonwebtoken": "^8.1.0",
    "linebyline": "^1.3.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "morgan": "^1.9.1",
    "pg": "^8.7.1",
    "processenv": "^1.1.0",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.9",
    "rxjs": "^6.6.7",
    "sequelize": "^6.6.5",
    "sqlite3": "^5.0.2",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/app-root-dir": "^0.1.0",
    "@types/compression": "0.0.36",
    "@types/config": "0.0.34",
    "@types/cors": "^2.8.12",
    "@types/express": "4.17.11",
    "@types/express-serve-static-core": "4.17.19",
    "@types/serve-static": "1.13.9",
    "@types/express-jwt": "^0.0.40",
    "@types/fs-readfile-promise": "^3.0.1",
    "@types/helmet": "0.0.42",
    "@types/jsonwebtoken": "^7.2.5",
    "@types/lodash": "^4.14.172",
    "@types/morgan": "^1.9.3",
    "@types/node": "^10.17.60",
    "@types/reflect-metadata": "^0.1.0",
    "@types/request": "^2.48.7",
    "@types/request-promise-native": "^1.0.18",
    "@types/sqlite3": "^3.1.7",
    "@types/swagger-ui-express": "^3.0.0",
    "@types/validator": "^13.6.3",
    "copyfiles": "^2.4.1",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^1.4.0",
    "rimraf": "^3.0.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^3.9.10"
  },
  "znConfig": {
    "lastChange": "2020-06-11 11:25:17 +0200"
  }
}
